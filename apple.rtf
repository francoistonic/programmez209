\f0\fs18 \cf0 program Perfs;\
\
    uses applestuff, transcend;\
    \
procedure RealFact;\
    var n, i, e : integer;\
        fact, fract : real;\
begin\
    Note(100, 30);\
    n := 300;\
    fact := 0;\
    for i:=2 to n do\
        fact := fact + Log(i);\
    write(n);\
    write('! = ');\
    e := trunc(fact);\
    fract := fact - e;\
    write(exp(fract * ln(10)));\
    write('E');\
    writeln(e);\
    Note(100, 30);\
end;\
\
begin\
    RealFact;\
end.\
