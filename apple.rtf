\f0\fs18 \cf0 program Selection;\
\
uses applestuff;\
\
var woody : text;\
    i,j,n,best : integer;\
    lines : array[1..100] of ^string;\
    temp : ^string;\
\
begin\
    note(100, 50);\
    \{ Turn off error checking so that program will do it \}\
    (*$I-*)\
    reset(woody, 'woody.text');\
    if ioresult <> 0 then begin\
        writeln('Unable to open file');\
        exit(program);\
    end;\
    \{ Turn on error checking \}\
    (*$I+*)\
    n := 1;\
    while not(eof(woody)) do\
    begin\
        new(lines[n]);\
        readln(woody, lines[n]^);\
        n := n + 1;\
    end;\
    n := n - 1;\
    write(n);\
    writeln(' lines');\
    note(100, 50);\
    for i :=1 to n-1 do\
    begin\
        best := i;\
        for j:=best+1 to n do\
            if lines[j]^ < lines[best]^ then\
                best := j;\
        if best <> i then\
        begin\
            temp := lines[i];\
            lines[i] := lines[best];\
            lines[best] := temp;\
        end;\
    end;\
    note(100, 50);\
    for i :=1 to n do\
        writeln(lines[i]^);\
    note(100, 50);\
end.\
