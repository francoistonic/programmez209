program MemorySample;\
\
type list=^node;\
     node = record\
         alloc : integer;\
         memory : integer;\
         next : list;\
     end;\
\
var i : integer;\
    head, tail, temp : list;\
\
begin\
    head := nil;\
    tail := nil;\
    for i:=1 to 5 do\
    begin\
        new(temp);\
        temp^.alloc := i;\
        temp^.memory := memavail;\
        temp^.next := nil;\
        if head = nil then head := temp;\
        if tail <> nil then\
            tail^.next := temp;\
        tail := temp;\
    end;\
    temp := head;\
    while temp <> nil do\
    begin\
        write('After ');\
        write(temp^.alloc);\
        write(' allocations, available memory = ');\
        writeln(temp^.memory);\
        temp := temp^.next;\
    end;\
end.\
