\f0\fs18 \cf0 program Factorial;\
\
type long_integer = integer[30];\
\
procedure Calc(n : integer;var f : long_integer);\
  var i : integer;\
begin\
  f := 1;\
  for i:=1 to n do\
    f := i * f;\
end;\
\
procedure FactTest;\
   var number : integer;\
   fact : long_integer;\
begin\
  repeat\
  begin\
    Write('n=?');\
    Read(number);\
    if number > 0 then\
    begin\
      Calc(number, fact);\
      write(number);\
      write('! = ');\
      writeln(fact);\
    end;\
  end\
  until number = 0;\
end;\
\
(* Entry Point *)\
begin\
  FactTest;\
end.\
